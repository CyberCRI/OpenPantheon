{
    "name": "open-pantheon",
    "version": "0.1.0",
    "private": true,
    "description": "The Open Pantheon for the education",
    "author": {
        "name": "CRI IT team"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "format": "prettier --write src/**/*.{json,js,ts,jsx,tsx}",
        "format:check": "prettier --check src/**/*.{json,js,ts,jsx,tsx}",
        "prepare": "husky install"
    },
    "dependencies": {
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/helper-regex": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-json-strings": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-import-meta": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.9.5",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "^1.0.3",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.27.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^7.10.0",
        "husky": "^7.0.0",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.2",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "vue-cli-plugin-i18n": "~2.1.1",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 11",
        "not dead"
    ],
    "engines": {
        "node": ">12.0.0 <15.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "license": "ISC",
    "lint-staged": {
        "**/*.{json,js,ts,tsx,vue}": [
            "yarn format",
            "git add"
        ],
        "*.{scss,css,html,yml,json}": "prettier --write"
    },
    "readme": "https://github.com/CyberCRI/OpenPantheon/blob/main/README.md"
}
